{
  "name": "dinky.js",
  "version": "0.8.1",
  "description": "JavaScript bindings for Derpibooru API",
  "main": "lib/Dinky",
  "repository": "https://github.com/octet-stream/dinky",
  "author": "Nick K. <io@octetstream.me>",
  "license": "MIT",
  "engines": {
    "node": ">=8 < 9 || >= 10 < 11 || >= 12"
  },
  "keywords": [
    "api-bindings",
    "derpibooru",
    "api-client"
  ],
  "scripts": {
    "test": "ava",
    "report": "nyc npm test",
    "report:html": "nyc npm test && nyc report --reporter=html",
    "report:lcov": "nyc npm test && nyc report --reporter=lcov",
    "ci": "npm run report:lcov && node_modules/.bin/codecov",
    "eslint": "eslint readme.md lib/**/*.js test/**/*.js",
    "staged": "lint-staged"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/helper/**"
    ]
  },
  "devDependencies": {
    "@octetstream/eslint-config": "4.0.0",
    "ava": "2.4.0",
    "codecov": "3.6.5",
    "eslint": "6.7.1",
    "eslint-plugin-ava": "9.0.0",
    "eslint-plugin-markdown": "1.0.1",
    "fetch-mock": "8.0.0",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "nyc": "14.1.1",
    "proxyquire": "2.1.3",
    "sinon": "7.5.0"
  },
  "dependencies": {
    "camelcase-keys": "6.1.1",
    "node-fetch": "2.6.0"
  }
}

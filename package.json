{
  "name": "dinky.js",
  "version": "1.0.0",
  "description": "JavaScript bindings for Derpibooru API",
  "main": "lib/Dinky",
  "types": "lib/Dinky.d.ts",
  "repository": "octet-stream/dinky",
  "author": "Nick K. <io@octetstream.me>",
  "license": "MIT",
  "engines": {
    "node": ">= 10 < 11 || >= 12"
  },
  "keywords": [
    "api-bindings",
    "derpibooru",
    "api-client"
  ],
  "scripts": {
    "test": "ava",
    "report": "nyc npm test",
    "report:html": "nyc npm test && nyc report --reporter=html",
    "report:lcov": "nyc npm test && nyc report --reporter=lcov",
    "ci": "npm run report:lcov && node_modules/.bin/codecov",
    "eslint": "eslint readme.md lib/**/*.js test/**/*.js",
    "staged": "lint-staged"
  },
  "devDependencies": {
    "@octetstream/eslint-config": "4.0.0",
    "ava": "3.8.2",
    "codecov": "3.7.0",
    "eslint": "7.0.0",
    "eslint-plugin-ava": "10.3.0",
    "eslint-plugin-markdown": "1.0.2",
    "fetch-mock": "9.9.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.4",
    "nyc": "15.0.1",
    "proxyquire": "2.1.3",
    "sinon": "9.0.2",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/node-fetch": "2.*",
    "camelcase-keys": "6.2.2",
    "node-fetch": "2.6.0"
  }
}
